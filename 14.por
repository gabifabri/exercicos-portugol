programa {
  funcao inicio() {
  caracter sexo
  escreva("Digite 'f' pra feminino ou 'm' pra masculino: " )
  leia(sexo)
  se(sexo=="f" ou sexo=="F"){
    escreva("seu sexo e feminino")
  }
  senao se(sexo=="m" ou sexo == "M"){
  escreva("seu sexo e masculino")
  }
  senao{
    escreva("sexo invalido")
   }
  }
}
